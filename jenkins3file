pipeline {
    agent any

    environment {
        AWS_DEFAULT_REGION = 'eu-north-1'
        S3_BUCKET = 'mystatics3bucket1993'
    }

    stages {
        stage('Clone Repo') {
            steps {
                git 'https://github.com/varaprasad070693/git-project.git'
            }
        }

        stage('Deploy to S3') {
            steps {
                sh 'aws s3 sync . s3://$S3_BUCKET'
            }
        }
    }
}
